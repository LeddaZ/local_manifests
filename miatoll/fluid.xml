<?xml version="1.0" encoding="UTF-8"?>
<manifest>

    <!-- Fluid 2.x Soda manifest for miatoll -->

    <!-- External remotes -->
    <remote name="github" fetch="https://github.com/" />
    <remote name="gitlab" fetch="https://gitlab.com/" />

    <!-- Projects to remove -->
    <remove-project name="build_soong" />
    <remove-project name="frameworks_base" />
    <remove-project name="vendor_gapps" />

    <!-- Device trees -->
    <project path="device/xiaomi/miatoll" name="LeddaZ/android_device_xiaomi_miatoll" revision="fluid-12" remote="github" />
    <project path="device/xiaomi/sm6250-common" name="LeddaZ/android_device_xiaomi_sm6250-common" revision="fluid-12" remote="github" />

    <!-- Kernel source -->
    <project path="kernel/xiaomi/sm6250" name="LeddaZ/android_kernel_xiaomi_sm6250" revision="pizza" remote="github" />

    <!-- Vendor blobs -->
    <project path="vendor/xiaomi/miatoll" name="LeddaZ/android_vendor_xiaomi_miatoll" revision="R" remote="github" />
    <project path="vendor/xiaomi/sm6250-common" name="LeddaZ/android_vendor_xiaomi_sm6250-common" revision="android-12" remote="github" />

    <!-- hardware/xiaomi from xiaomi-sm8250-devs -->
    <project path="hardware/xiaomi" name="xiaomi-sm8250-devs/android_hardware_xiaomi" revision="lineage-19.0" remote="github" />

    <!-- Repos from me -->
    <project path="build/soong" name="LeddaZ/fluid_build_soong" revision="fluid-12" remote="github">
      <linkfile src="root.bp" dest="Android.bp" />
      <linkfile src="bootstrap.bash" dest="bootstrap.bash" />
    </project>
    <project path="frameworks/base" name="LeddaZ/fluid_frameworks_base" revision="fluid-12" remote="github" />
    <project path="vendor/gapps" name="LeddaZ/vendor_gapps" revision="twelve" remote="gitlab" />

</manifest>
