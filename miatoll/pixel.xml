<?xml version="1.0" encoding="UTF-8"?>
<manifest>

    <!-- PixelOS 12 manifest for miatoll -->

    <!-- Projects to remove -->
    <remove-project name="build" />
    <remove-project name="build_soong" />
    <remove-project name="frameworks_base" />
    <remove-project name="frameworks_native" />
    <remove-project name="hardware_custom_interfaces" />
    <remove-project name="hardware_ril" />
    <remove-project name="system_core" />
    <remove-project name="vendor_aosp" />
    <remove-project name="vendor_qcom_opensource_bluetooth-commonsys-intf" />
    <remove-project name="vendor_qcom_opensource_system_bt" /> 
    <remove-project name="vendor_qcom_opensource_packages_apps_Bluetooth" />
    <remove-project name="vendor_qcom_opensource_power" />
    <remove-project name="vendor_qcom_opensource_bluetooth_ext" />
    <remove-project name="vendor_qcom_opensource_audio-hal_st-hal" />

    <!-- Device trees -->
    <project path="device/xiaomi/miatoll" name="LeddaZ/android_device_xiaomi_miatoll" remote="github" revision="twelve" />
    <project path="device/xiaomi/sm6250-common" name="LeddaZ/android_device_xiaomi_sm6250-common" remote="github" revision="twelve" />

    <!-- Kernel source -->
    <project path="kernel/xiaomi/sm6250" name="LeddaZ/android_kernel_xiaomi_sm6250" remote="github" revision="pizza" />

    <!-- Vendor blobs -->
    <project path="vendor/xiaomi/miatoll" name="LeddaZ/android_vendor_xiaomi_miatoll" remote="github" revision="twelve" />
    <project path="vendor/xiaomi/sm6250-common" name="LeddaZ/android_vendor_xiaomi_sm6250-common" remote="github" revision="twelve" />

    <!-- hardware/xiaomi from xiaomi-sm8250-devs -->
    <project path="hardware/xiaomi" name="xiaomi-sm8250-devs/android_hardware_xiaomi" remote="github" revision="lineage-19.0" />

    <!-- Repos from LineageOS -->
    <project path="hardware/lineage/interfaces" name="LineageOS/android_hardware_lineage_interfaces" revision="lineage-19.0" remote="github" />
   
    <!-- Repos from PixelOS -->
    <project path="build/make" name="PixelOS-Devices/build" remote="github" revision="twelve">
      <copyfile src="core/root.mk" dest="Makefile" />
      <linkfile src="CleanSpec.mk" dest="build/CleanSpec.mk" />
      <linkfile src="buildspec.mk.default" dest="build/buildspec.mk.default" />
      <linkfile src="core" dest="build/core" />
      <linkfile src="envsetup.sh" dest="build/envsetup.sh" />
      <linkfile src="target" dest="build/target" />
      <linkfile src="tools" dest="build/tools" />
    </project>
    <project path="build/soong" name="LeddaZ/build_soong" remote="github" revision="twelve">
      <linkfile src="root.bp" dest="Android.bp" />
      <linkfile src="bootstrap.bash" dest="bootstrap.bash" />
    </project>
    <project path="frameworks/native" name="PixelOS-Devices/frameworks_native" revision="twelve" remote="github" />
    <project path="system/core" name="PixelOS-Devices/system_core" revision="twelve" remote="github" />

    <!-- Repos from me -->
    <project path="frameworks/base" name="LeddaZ/frameworks_base" revision="twelve" remote="github" />
    <project path="hardware/ril" name="LeddaZ/hardware_ril" revision="twelve" remote="github" />
    <project path="vendor/aosp" name="LeddaZ/vendor_aosp" revision="twelve" remote="github" />
    <project path="vendor/qcom/opensource/power" name="LeddaZ/vendor_qcom_opensource_power" revision="twelve" remote="github" />

</manifest>
